public class BucleIF2 {


	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int num=17;
		
		//OPERADOR LÃ“GICO AND
		
		if(num>=10 && num<15) {
			System.out.println("El nÃºmero es mayor o igual que 10 y menor que 15");
		}else {
			System.out.println("El numero es menor que 10");
		}
		
		//OPERADOR LÃ“GICO OR
		
		if(num>=10 || num<15) {
			System.out.println("El nÃºmero es mayor o igual que 10 o menor que 15");
		}else {
			System.out.println("El numero es menor que 10");
		}
	}

}
